GET     /                           controllers.TopController.show()

GET     /signin                      controllers.SigninController.index()
POST    /signin                      controllers.SigninController.signin()

GET    /signup                      controllers.SignupController.index()
POST   /signup                      controllers.SignupController.signup()

GET     /readSession                 controllers.SigninController.getSession

GET /assets/*file                   controllers.Assets.versioned(path="/public", file: Asset)
